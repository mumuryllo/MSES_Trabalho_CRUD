package br.Model;

    public class Ipva {
	private int idIPVA;
	private String anoIPVA;
	
	public Ipva(String anoIPVA) {
		super();
		this.anoIPVA = anoIPVA;
	}


	public Ipva(int idIPVA) {
		super();
		this.idIPVA = idIPVA;
	}

	
	
	public int getIdIPVA() {
		return idIPVA;
	}


	public void setIdIPVA(int idIPVA) {
		this.idIPVA = idIPVA;
	}


	public String getAnoIPVA() {
		return anoIPVA;
	}


	public void setAnoIPVA(String anoIPVA) {
		this.anoIPVA = anoIPVA;
	}



	

}
