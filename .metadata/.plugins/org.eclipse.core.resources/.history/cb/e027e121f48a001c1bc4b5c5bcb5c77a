package br.Model;

public class Carros {

private Integer id;
private String ano;
private String modelo;

public Carros(String ano, String modelo) {
	super();
	this.ano = ano;
	this.modelo = modelo;
}


public Carros(Integer id) {
	super();
	this.id = id;
}
public Integer getId() {
	return id;
}
public void setId(Integer id) {
	this.id = id;
}
public String getAno() {
	return ano;
}
public void setAno(String ano) {
	this.ano = ano;
}
public String getModelo() {
	return modelo;
}
public void setModelo(String modelo) {
	this.modelo = modelo;
}

public String toString() {
	return "Carros [id=" + id + ", ano=" + ano + ", modelo=" + modelo + "]";
}
}
